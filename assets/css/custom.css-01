.carousel-fade {
  .carousel-inner {
    .carousel-item {
      opacity: 0  !important;
      transition-property: opacity !important;
    }

    .active {
      opacity: 1  !important;
    }

    .active.left,
    .active.right {
      left: 0  !important;
      opacity: 0 !important;
      z-index: 1 !important;
    }

    .next.left,
    .prev.right {
      opacity: 1 !important;
    }
  }

  .carousel-control {
    z-index: 2 !important;
  }
}

