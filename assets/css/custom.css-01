/*-----------------*/
/***** Apollo *****/
/*-----------------*/

figure.effect-apollo {
  margin: 0px !important;
}

figure.effect-apollo img {
  opacity: 0.95;
  -webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
  transition: opacity 0.35s, transform 0.35s;
  -webkit-transform: scale3d(1.05,1.05,1);
  transform: scale3d(1.05,1.05,1);
}

figure.effect-apollo figcaption::before {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(255,255,255,0.2);
  content: '';
  -webkit-transition: -webkit-transform 0.6s;
  transition: transform 0.6s;
  -webkit-transform: scale3d(1.9,1.4,1) rotate3d(0,0,1,45deg) translate3d(0,-100%,0);
  transform: scale3d(1.9,1.4,1) rotate3d(0,0,1,45deg) translate3d(0,-100%,0);
}

figure.effect-apollo p {
  position: absolute;
  right: 0;
  bottom: 0;
  margin: 3em;
  padding: 0 1em;
  max-width: 150px;
  border-right: 4px solid #fff;
  text-align: right;
  opacity: 0;
  -webkit-transition: opacity 0.35s;
  transition: opacity 0.35s;
}

figure.effect-apollo h2 {
  text-align: left;
}

figure.effect-apollo:hover img {
  opacity: 0.9;
  -webkit-transform: scale3d(1,1,1);
  transform: scale3d(1,1,1);
}

figure.effect-apollo:hover figcaption::before {
  -webkit-transform: scale3d(1.9,1.4,1) rotate3d(0,0,1,45deg) translate3d(0,100%,0);
  transform: scale3d(1.9,1.4,1) rotate3d(0,0,1,45deg) translate3d(0,100%,0);
}

figure.effect-apollo:hover p {
  opacity: 1;
  -webkit-transition-delay: 0.1s;
  transition-delay: 0.1s;
}
.apollo-holder{
  position: relative;
  max-width: 100%;
  overflow: hidden;
}
.apollo-holder figure {
  position: relative;
  float: left;
  overflow: hidden;
  min-width: 120px;
  max-width: 100%;
  max-height: 100%;
  width: 100%;
  height: auto;
  text-align: center;
  cursor: pointer;
}

.apollo-holder figure img {
  position: relative;
  display: block;
  height: 100%;
  width: 100%;
  opacity: 1;
}

.apollo-holder figure figcaption {
  padding: 2em;
  color: #fff;
  text-transform: uppercase;
  font-size: 1.25em;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  font-weight: 600;
  font-family: roboto;
}

.apollo-holder figure figcaption::before,
.apollo-holder figure figcaption::after {
  pointer-events: none;
}

.apollo-holder figure figcaption,
.apollo-holder figure figcaption > a {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
/*  object-position: 100% 100%  !important;
*/}
.bg-blue .apollo-holder img{
  object-fit: contain !important;
  width: 100% !important
}
